\";\n\nimport ShippingInfoForm from \"./ShippingInfoForm\";\nimport Payment from \"./Payment\";\nimport Confirmation from \"./Confirmation\";\nimport Message from \"../../components/Message\";\n\nexport default function Checkout(props) {\n  const checkoutStep = props.match.params.step;\n  return (\n    <div class=\"checkout\">\n      {checkoutStep == \"shipping-info\" ? (\n        <ShippingInfoForm history={props.history} />\n      ) : checkoutStep == \"payment\" ? (\n        <Payment />\n      ) : checkoutStep == \"confirmation\" ? (\n        <Confirmation history={props.history} />\n      ) : (\n        <Message>Page Not Found</Message>\n      )}\n    </div>\n  );\n}\n","/Users/hannah/Programming/raw intentions/raw-intentions/src/routes/cart/Cart.js",["156","157","158","159","160","161","162"],"/Users/hannah/Programming/raw intentions/raw-intentions/src/constants/constants.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/constants/cartConstants.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/components/Slideshow.js",["163"],"import React from \"react\";\nimport { Slide } from \"react-slideshow-image\";\nimport Neutrals from \".././assets/slideshow/neutrals.png\";\nimport Basics from \".././assets/slideshow/basics.png\";\nimport Summer from \".././assets/slideshow/welcome-to-summer.png\";\n\nconst images = [Neutrals, Basics, Summer];\nconst properties = {\n  duration: 4000,\n  transitionDuration: 700,\n  infinite: true,\n  arrows: false,\n  pauseOnHover: true,\n};\nconst Slideshow = () => {\n  return (\n    <div className=\"slide-container\">\n      <Slide {...properties}>\n        {images.map((image) => (\n          <div className=\"each-slide\">\n            <img src={image}></img>\n          </div>\n        ))}\n      </Slide>\n    </div>\n  );\n};\n\nexport default Slideshow;\n","/Users/hannah/Programming/raw intentions/raw-intentions/src/constants/productConstants.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/components/Message.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/routes/checkout/ShippingInfoForm.js",["164","165"],"/Users/hannah/Programming/raw intentions/raw-intentions/src/routes/checkout/Payment.js",["166"],"import React from \"react\";\nimport { useSelector } from \"react-redux\";\n\nexport default function Payment() {\n  const cart = useSelector((state) => state.cart);\n  const { cartItems, shippingInfo } = cart;\n  const totalPrice = (obj) =>\n    Object.values(obj).reduce((a, b) => a + b.quantity * b.price, 0);\n\n  return (\n    <div className=\"payment\">\n      <h1>Payment</h1>\n      <div className=\"payment-info\">\n        <p>\n          Thanks for placing your order! Your order information has been\n          submitted and will be processed shortly{\" \"}\n          <strong>once you follow the instructions below:</strong>\n        </p>\n        <ol>\n          <li>\n            Please send an e-Transfer to <em>rawintentionsco@gmail.com</em> of\n            your total of{\" \"}\n            <span>\n              $\n              {cartItems.length == 0\n                ? \"0.00\"\n                : totalPrice(cartItems).toFixed(2)}\n            </span>{\" \"}\n            with your order number <span>{shippingInfo.orderId}</span> in the\n            message.{\" \"}\n            <strong>\n              E-Transfers without an order number will not be processed.\n            </strong>\n          </li>\n          <li>\n            For the security question, please use the following combination:\n            <div className=\"e-transfer-question\">\n              <p>Q: What did I buy from you?</p>\n              <p>A: Jewelry</p>\n            </div>\n          </li>\n          <li>\n            Once the e-Transfer has been received, we will begin creating the\n            order. In order to maintain the integrity of our small business,{\" \"}\n            <strong>\n              your e-Transfer will not be deposited until the order has shipped.\n            </strong>\n          </li>\n          <li>\n            Once your order has shipped, the e-Transfer will be deposited and\n            you're on track to receive your package in the mail!\n          </li>\n        </ol>\n        <p className=\"disclaimer\">\n          * To lower shipping costs, orders are sent using untracked shipping\n        </p>\n        <p className=\"disclaimer\">\n          *If information or funds are missing, your order will NOT be processed\n          and you may be contacted\n        </p>\n      </div>\n    </div>\n  );\n}\n","/Users/hannah/Programming/raw intentions/raw-intentions/src/actions/productActions.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/actions/cartActions.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/components/Popup.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/routes/information/AboutUs.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/routes/information/FAQ.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/routes/information/Shipping.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/routes/information/ContactUs.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/components/Dropdown.js",[],"/Users/hannah/Programming/raw intentions/raw-intentions/src/routes/checkout/Confirmation.js",["167","168","169","170","171"],"import React, { useState } from \"react\";\nimport emailjs from \"emailjs-com\";\nimport { useSelector } from \"react-redux\";\nimport { NavLink } from \"react-router-dom\";\nimport Message from \"../../components/Message\";\n\nexport default function Confirmation(props) {\n  const cart = useSelector((state) => state.cart);\n  const { cartItems, shippingInfo } = cart;\n\n  const totalQuantity = (obj) =>\n    Object.values(obj).reduce((a, b) => a + b.quantity, 0);\n  const totalPrice = (obj) =>\n    Object.values(obj).reduce((a, b) => a + b.quantity * b.price, 0);\n\n  const EMAILJS_SERVICE_ID = process.env.REACT_APP_EMAILJS_SERVICE_ID;\n  const EMAILJS_TEMPLATE_ID = process.env.REACT_APP_EMAILJS_TEMPLATE_ID;\n  const EMAILJS_USER_ID = process.env.REACT_APP_EMAILJS_USER_ID;\n\n  const [sent, setSent] = useState(false);\n\n  const submitHandler = (e) => {\n    var templateParams = {\n      cartItems: JSON.stringify(cartItems),\n      ...shippingInfo,\n      total: `$${totalPrice(cartItems).toFixed(2)}`,\n    };\n    emailjs\n      .send(\n        EMAILJS_SERVICE_ID,\n        EMAILJS_TEMPLATE_ID,\n        templateParams,\n        EMAILJS_USER_ID\n      )\n      .then(\n        function (response) {\n          console.log(\"SUCCESS!\", response.status, response.text);\n        },\n        function (error) {\n          console.log(\"FAILED...\", error);\n        }\n      );\n    setSent(true);\n    props.history.push(\"payment\");\n  };\n  return (\n    <div className=\"confirmation\">\n      <h1>Order Confirmation</h1>\n      <div className=\"confirmation-shipping-info\">\n        <h2>Shipping Information</h2>\n        <NavLink\n          to=\"/checkout/shipping-info\"\n          className=\"confirmation-edit-info\"\n        >\n          Edit\n        </NavLink>\n        <table className=\"confirmation-form-info\">\n          <tr>\n            <th>First Name</th>\n            <td>{shippingInfo.firstName}</td>\n          </tr>\n          <tr>\n            <th>Last Name</th>\n            <td>{shippingInfo.lastName}</td>\n          </tr>\n          <tr>\n            <th>Email</th>\n            <td>{shippingInfo.email}</td>\n          </tr>\n          <tr>\n            <th>Phone</th>\n            <td>{shippingInfo.phoneNumber}</td>\n          </tr>\n          <tr>\n            <th>Street Address</th>\n            <td>{shippingInfo.streetAddress}</td>\n          </tr>\n        </table>\n        <table className=\"confirmation-form-info\">\n          <tr>\n            <th>Country</th>\n            <td>{shippingInfo.country}</td>\n          </tr>\n          <tr>\n            <th>Province</th>\n            <td>{shippingInfo.state}</td>\n          </tr>\n          <tr>\n            <th>City</th>\n            <td>{shippingInfo.city}</td>\n          </tr>\n          <tr>\n            <th>Postal Code</th>\n            <td>{shippingInfo.postalCode}</td>\n          </tr>\n          <tr>\n            <th>Apt Suite</th>\n            <td>{shippingInfo.aptSuite}</td>\n          </tr>\n        </table>\n      </div>\n      <div className=\"confirmation-cart-info\">\n        <div className=\"cart-items\">\n          {cartItems.length == 0 ? (\n            <Message>\n              NOTHING HERE!\n              <br />\n              <NavLink to=\"/shop\">SHOP NOW</NavLink>\n            </Message>\n          ) : (\n            cartItems.map((item) => (\n              <div className=\"cart-item\">\n                <NavLink to={`/product/${item.product}`}>\n                  <img src={item.image} />\n                  <div className=\"cart-item-info\">\n                    <h2>\n                      {item.name} - {item.type}\n                    </h2>\n                    <p>SIZE {item.size}</p>\n                    <p>\n                      ${item.price.toFixed(2)} X {item.quantity}\n                    </p>\n                  </div>\n                </NavLink>\n                <div className=\"cart-item-price\">\n                  <h2>${(item.quantity * item.price).toFixed(2)}</h2>\n                  <NavLink to=\"/cart\" className=\"confirmation-edit-info\">\n                    Edit\n                  </NavLink>\n                </div>\n              </div>\n            ))\n          )}\n        </div>\n        <div className=\"cart-summary\">\n          <h2>Summary</h2>\n          <div className=\"cart-summary-details\">\n            <span>Items</span>\n            <span>{cartItems.length == 0 ? 0 : totalQuantity(cartItems)}</span>\n          </div>\n          <div className=\"cart-summar