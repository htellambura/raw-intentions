>\n        <h2>Shipping Information</h2>\n        <NavLink\n          to=\"/checkout/shipping-info\"\n          className=\"confirmation-edit-info\"\n        >\n          Edit\n        </NavLink>\n        <table className=\"confirmation-form-info\">\n          <tr>\n            <th>First Name</th>\n            <td>{shippingInfo.firstName}</td>\n          </tr>\n          <tr>\n            <th>Last Name</th>\n            <td>{shippingInfo.lastName}</td>\n          </tr>\n          <tr>\n            <th>Email</th>\n            <td>{shippingInfo.email}</td>\n          </tr>\n          <tr>\n            <th>Phone</th>\n            <td>{shippingInfo.phoneNumber}</td>\n          </tr>\n          <tr>\n            <th>Street Address</th>\n            <td>{shippingInfo.streetAddress}</td>\n          </tr>\n        </table>\n        <table className=\"confirmation-form-info\">\n          <tr>\n            <th>Country</th>\n            <td>{shippingInfo.country}</td>\n          </tr>\n          <tr>\n            <th>Province</th>\n            <td>{shippingInfo.state}</td>\n          </tr>\n          <tr>\n            <th>City</th>\n            <td>{shippingInfo.city}</td>\n          </tr>\n          <tr>\n            <th>Postal Code</th>\n            <td>{shippingInfo.postalCode}</td>\n          </tr>\n          <tr>\n            <th>Apt Suite</th>\n            <td>{shippingInfo.aptSuite}</td>\n          </tr>\n        </table>\n      </div>\n      <div className=\"confirmation-cart-info\">\n        <div className=\"cart-items\">\n          {cartItems.length == 0 ? (\n            <Message>\n              NOTHING HERE!\n              <br />\n              <NavLink to=\"/shop\">SHOP NOW</NavLink>\n            </Message>\n          ) : (\n            cartItems.map((item) => (\n              <div className=\"cart-item\">\n                <NavLink to={`/product/${item.product}`}>\n                  <img src={item.image} />\n                  <div className=\"cart-item-info\">\n                    <h2>\n                      {item.name} - {item.type}\n                    </h2>\n                    <p>SIZE</p> {/* implement size calculation */}\n                    <p>\n                      ${item.price.toFixed(2)} X {item.quantity}\n                    </p>\n                  </div>\n                </NavLink>\n                <div className=\"cart-item-price\">\n                  <h2>${(item.quantity * item.price).toFixed(2)}</h2>\n                  <NavLink to=\"/cart\" className=\"confirmation-edit-info\">\n                    Edit\n                  </NavLink>\n                </div>\n              </div>\n            ))\n          )}\n        </div>\n        <div className=\"cart-summary\">\n          <h2>Summary</h2>\n          <div className=\"cart-summary-details\">\n            <span>Items</span>\n            <span>{cartItems.length == 0 ? 0 : totalQuantity(cartItems)}</span>\n          </div>\n          <div className=\"cart-summary-details\">\n            <span>Shipping</span>\n            <span>$4.00</span>{\" \"}\n            {/* implement actual shipping cost calculation */}\n          </div>\n          <div className=\"cart-summary-details\">\n            <span>Total</span>\n            <span>\n              $\n              {cartItems.length == 0\n                ? \"0.00\"\n                : totalPrice(cartItems).toFixed(2)}\n            </span>\n          </div>\n        </div>\n      </div>\n      <button type=\"submit\" onClick={(e) => submitHandler()}>\n        <h3>Confirm my Order</h3>\n      </button>\n    </div>\n  );\n}\n","/Users/hannah/Programming/raw intentions/raw-intentions/src/components/Filter.js",["164"],"/Users/hannah/Programming/raw intentions/raw-intentions/src/components/Sort.js",[],{"ruleId":"165","replacedBy":"166"},{"ruleId":"167","replacedBy":"168"},{"ruleId":"169","severity":1,"message":"170","line":11,"column":63,"nodeType":"171","messageId":"172","endLine":11,"endColumn":65},{"ruleId":"169","severi